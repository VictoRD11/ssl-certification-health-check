{
  "name": "victord11/ssl-certification-health-check",
  "description": "A Laravel Health check to SSL certification",
  "keywords": [
    "victord11",
    "laravel",
    "spatie",
    "ssl-certification-health-check"
  ],
  "homepage": "https://github.com/victord11/ssl-certification-health-check",
  "license": "MIT",
  "authors": [
    {
      "name": "Viktor Mazur",
      "email": "victord11@ukr.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "spatie/ssl-certificate": "^2.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "pestphp/pest": "^2.12",
    "pestphp/pest-plugin-laravel": "^2.3",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpunit/phpunit": "^10.0",
    "spatie/laravel-health": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "VictoRD11\\SslCertificationHealthCheck\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VictoRD11\\SslCertificationHealthCheck\\Tests\\": "tests"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php --allow-risky=yes"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
